<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tubesakb.OnlineBook">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme2">
        <activity android:name="com.tubesakb.OnlineBook.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tubesakb.OnlineBook.MainActivity"
            android:label="Dashboard"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.tubesakb.OnlineBook.Informasi"
            android:label="Informasi"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.tubesakb.OnlineBook.InputData"
            android:label="Input data pesanan"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.tubesakb.OnlineBook.DataPesanan"
            android:label="Data pesanan"
            android:theme="@style/AppTheme" />
        <activity android:name="com.tubesakb.OnlineBook.DetailData" />
        <activity android:name="com.tubesakb.OnlineBook.UpdateData"></activity>
    </application>

</manifest>